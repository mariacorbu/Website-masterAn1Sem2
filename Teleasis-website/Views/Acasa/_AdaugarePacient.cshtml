@model Teleasis_website.Models.PacientModel 
<div class="mb-3">
    <label for="recipient-name" class="col-form-label">Nume:</label>
    <input type="text" class="form-control" id="input-nume-pacient" name="nume_pacient"/>
</div>
<div class="mb-3">
    <label for="message-text" class="col-form-label">Prenume:</label>
    <input type="text" class="form-control" id="input-prenume-pacient" name="prenume_pacient"/>
</div>
<div class="mb-3">
    <label for="message-text" class="col-form-label">CNP:</label>
    <input type="number" class="form-control" id="input-cnp" name="CNP"/>
</div>
<div class="mb-3">
    <label for="message-text" class="col-form-label">Email:</label>
    <input type="email" class="form-control" id="input-email-pacient" name="email_pacient" />
</div>
<div class="mb-3">
    <label for="message-text" class="col-form-label">Parola:</label>
    <input class="form-control" type="password" id="input-parola-pacient" name="parola_pacient" />
</div>