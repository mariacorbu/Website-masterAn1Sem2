@model Teleasis_website.Models.SupraveghetorModel
<div class="mb-3">
    <label for="recipient-name" class="col-form-label">Nume:</label>
    <input type="text" class="form-control" id="input-nume-supraveghetor" name="nume_supraveghetor"/>
</div>
<div class="mb-3">
    <label for="message-text" class="col-form-label">Prenume:</label>
    <input type="text" class="form-control" id="input-prenume-supraveghetor" name="prenume_supraveghetor"/>
</div>
<div class="mb-3">
    <label for="message-text" class="col-form-label">Email:</label>
    <input type="email" class="form-control" id="input-email-supraveghetor" name="email_supraveghetor" />
</div>
<div class="mb-3">
    <label for="message-text" class="col-form-label">Parola:</label>
    <input class="form-control" type="password" id="input-parola-supraveghetor" name="parola_supraveghetor"/>
</div>
